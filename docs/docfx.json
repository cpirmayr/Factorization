{
  "metadata": [
    {
      "src": [
        {
          "files": ["../**/*.csproj"],
          "exclude": ["**/obj/**", "**/bin/**"]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/*.md"]
      },
      {
        "files": ["*.md"]
      }
    ],
    "dest": "_site",
    "template": ["default", "modern"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}